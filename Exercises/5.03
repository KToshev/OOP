#include <iostream>

using namespace std;

class Point2D
{
    private:
        double X,Y;

    public:
        void SetX(double);
        void SetY(double);
        double GetX() const;
        double GetY() const;
        void print() const;


};

double Point2D::GetX() const
{
    return X;
}

double Point2D::GetY() const
{
    return Y;
}

void Point2D::SetX(double val)
{
    X=val;
}

void Point2D::SetY(double val)
{
    Y=val;
}

void Point2D::print() const
{
    cout<<X<<" "<<Y<<endl;
}

int main()
{
    Point2D A;
    A.SetX(2);
    A.SetY(3);
    cout<<A.GetX()<<" "<<A.GetY()<<endl;
    A.print();
    return 0;
}
